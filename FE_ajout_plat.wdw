    name : bt_annuler
    identifier : 0x2308886102fca73f
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //Si l'utilisateur souhaite annuler la suppression de l'aliment alors il appuie sur le bouton annuler qui va annuler toutes les opérations effectuées sur le fichier de données
         HTransactionAnnule(MYSQLConnexion)
         //On actualise à nouveau pour ré afficher l'aliment "supprimé" auparavant
         TableDisplay(tb_ajout_aliment)
         
         //Ferme la fenêtre
         Close()
        type : 18
