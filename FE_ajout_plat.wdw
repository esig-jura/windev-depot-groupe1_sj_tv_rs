   -
     code : |1+
      PROCEDURE MaFenêtre()
   -
     code : |1+
      //Windev va contrôler si c'est un nouvel enregistrement 
      IF tb_plat.NewRecord THEN
      	//Si c'est un nouvel enregistrement Windev va réinitialiser tous les champs de la fenêtre à 0
      	Reset()
      ELSE
      	//Sinon il va afficher toutes les informations du plat contenu dans le champ de saisie (CS_ajout_plat)
      	FileToScreen()
      	//On donne au paramètre de la requête REQ_affiche_aliment la valeur présente dans le champ de saisie (CS_ajout_plat)
      	REQ_affiche_aliment.pata_id_plat	= tb_plat.id_plat
      	tb_ajout_aliment.BrowsedFile		= "REQ_affiche_aliment"
      	
      END
      
