   -
     name : Procédure_export
     procedure_id : 1371498910070658399
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Procédure_export ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCÉDURE Procédure_export(pfichier)
      nFichierID				is int			// identifiant du fichier créé
      LigneAecrire			is string
      
      
      // Dans la colonne fk_util_com je veux que il l’export la valeur afficher pas la fk 
      
      nFichierID = fOpen(pfichier, foCreate)
      IF nFichierID = -1 THEN
      	Error("Problème de création du fichier.")
      	ReturnToCapture()
      ELSE
      	FOR EACH ROW IN tb_commande
      		
      		LigneAecrire = Complète(tb_commande.id_com,10)+...
      		Complète(tb_commande.fk_util_com..ValeurAffichée,30)+...
      		Complète(tb_commande.etat_com,5)+...
      		Complète(tb_commande.date_com,10)
      		
      		fWriteLine(nFichierID, LigneAecrire)
      	END
      	
      	Info("données exporté avec succès")
      	// je crée qui une condition qui demande à l'utilisateur si il souhaite ouvrir le fichier exporter
      	IF YesNo(Yes,"Vous voulez ouvrir le fichier exporter ?") THEN
      		ShellExecute(pfichier)
      	END
      	
      END    
      
      	
      	
      
      
