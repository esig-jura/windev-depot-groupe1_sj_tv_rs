    name : bt_supprime_alim
    identifier : 0x2308731200cd6592
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         IF NOT HSeekFirst(tb_composer, fk_comp_alim, tb_aliment.COL_alim) THEN 
         	// demander confirmation
         	IF YesNo(No,"Voulez vraiment supprimer le composant" + RC + RC + tb_aliment.COL_alim) THEN
         		// Recherche dans la table des composants, sur la rubrique RefComp, 
         		// la valeur contenue dans RefComp de la table
         		HReadSeekFirst(tb_aliment, id_alim, tb_aliment.COL_alim) 
         		IF HFound THEN // Si la valeur est trouvée alors
         			HDelete(tb_aliment) // supprime la ligne dans le fichier Composant 
         			Info("Suppression réussie") // informe l'utilisateur suppression OK
         			Affichetable() 
         		ELSE // Sinon, on n'a pas trouvé ?!!
         			Info("Il y a un boulon dans le potage") // Bizarre, bizarre !
         		END
         	END
         ELSE
         	Error("Suppression impossible car ce composant est encore lié à un ensemble !")
         END
        type : 18
      -
