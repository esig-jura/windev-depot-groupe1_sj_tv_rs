  -
    name : bt_supprimer
    identifier : 0x23033f11081e9b14
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //Boîte de dialogue qui demande à l'utilisateur si celui-ci désire effacer l'enregistrement sélectionné 
         IF YesNo(No,"Voulez-vous supprimer cette localité ?") THEN
         	//Si oui alors on efface l'enregistrement de la table tb_localite
         	HDelete(tb_localite)
         	//En revanche, si une localité est encore lié à un utilisateur alors un message d'erreur viendra prévenir l'utilisateur qu'il ne peut pas supprimer l'enregistrement actuel
         	SI HErreurIntégrité() = Vrai ALORS
         		//Informe l'utilisateur si l'aliment sélectionné est encore relié à un plat
         		Info("Cette localité est encore reliée à au moins un utilisateur !")
         	FIN
         	//met l'affichage à jour en fonction de l'état de la table
         	TableDisplay(tb_localite)
         ELSE
         	//Si non,  une fenêtre informe l'utilisateur que la suppression a été abandonnée
         	Info("Suppression abandonnée")	
         END
        type : 18
      -
        type : 161
        enabled : false
