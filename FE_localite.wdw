        code : |1+
         //Boîte de dialogue qui demande à l'utilisateur si celui-ci désire effacer l'enregistrement sélectionné 
         IF YesNo(No,"Voulez-vous supprimer cette localité ?") THEN
         	//Si oui alors on efface l'enregistrement de la table tb_localite
         	HDelete(tb_localite)
         	//met l'affichage à jour en fonction de l'état de la table
         	TableDisplay(tb_localite)
         ELSE
