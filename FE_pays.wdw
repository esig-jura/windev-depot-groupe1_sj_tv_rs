         //Boîte de dialogue qui demande à l'utilisateur si celui-ci désire effacer l'enregistrement sélectionné 
         IF YesNo(No,"Voulez-vous supprimer ce pays ?") THEN
         	//Si oui alors on efface l'enregistrement de la table tb_pays
         	HDelete(tb_pays)
         	//met l'affichage à jour en fonction de l'état de la table
         	TableDisplay(tb_pays)
         ELSE
